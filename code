            Already_Spectating = True
            Spectator_Mode = "ON"
            Play_Again = input("Spectate, Play again? (S/Y/N): ").lower()
            if Play_Again == "y":
                Again()
            elif Play_Again == "s":
                Detective_Role()
            elif Play_Again == "n":
                exit()
            else:
                if Spectator_Mode == "OFF":
                    print("Invalid key")
        else:
            if Spectator_Mode == "OFF":
                print("Invalid key") 
        Detective_Role()
            

    def Traitor_Role():
        global Pos_x, Pos_y, Player, Bot_1_Pos_x, Bot_1_Pos_y, Bot_2_Pos_x, Bot_2_Pos_y, Bot_3_Pos_x, Bot_3_Pos_y, Bot_4_Pos_x, Bot_4_Pos_y, Bot_1_alive, Bot_2_alive, Bot_3_alive, Bot_4_alive, In, De, Tr, Player_alive,Spectator_Mode ,Already_Spectating, Move, Investigating_centre_Pos_x, Investigating_centre_Pos_y, Delivered_clue_1, Delivered_clue_2, Delivered_clue_3, Delivered_clue_4, Delivered_clue_5, Clue4_Pos_x, Clue4_Pos_y, Clue3_Pos_x, Clue3_Pos_y, Clue4, Clue3, Grabbed_clue_5, Grabbed_clue_4, Investigation_completed


        Bot1()
        Bot2()
        Bot3()
        Bot4()
        Check_for_win()
        
        if Bot_1_Pos_x == Pos_x and Bot_1_Pos_y == Pos_y and Bot_1_alive == True:
